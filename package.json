{
  "name": "dhanvantri",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run db:setup && next build",
    "db:setup": "npm run db:generate && npm run db:migrate:postgres",
    "db:generate": "npx prisma generate --schema=./prisma/mongo/schema.prisma && npx prisma generate --schema=./prisma/postgres/schema.prisma",
    "db:migrate:postgres": "npx prisma migrate deploy --schema=./prisma/postgres/schema.prisma",
    "db:push:mongo": "npx prisma db push --schema=./prisma/mongo/schema.prisma",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@vercel/analytics": "^1.5.0",
    "formidable": "^3.5.4",
    "geist": "^1.5.1",
    "jotai": "^2.14.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "openai": "^5.21.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-resizable-panels": "^3.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}